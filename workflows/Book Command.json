        "parameters": {
          "jsCode": "// Parse the booking JSON from arg1\nconst input = $input.first().json;\nconst chat_id = input.chat_id;\nconst lang = input.lang || 'en';\n\ntry {\n  const bookingsJson = input.arg1;\n  \n  if (!bookingsJson) {\n    throw new Error('No booking data provided');\n  }\n  \n  const bookings = JSON.parse(bookingsJson);\n  \n  if (!Array.isArray(bookings) || bookings.length === 0) {\n    throw new Error('Booking data must be a non-empty array');\n  }\n  \n  const validatedBookings = bookings.map((booking, index) => {\n    if (!booking.court || !booking.date || !booking.time_start || !booking.time_end || !booking.players_num) {\n      throw new Error(`Booking ${index + 1} missing required fields`);\n    }\n    \n    let dateStr = booking.date;\n    let dateMatch = dateStr.match(/^(\\d{1,2})\\.(\\d{1,2})\\.(\\d{2,4})$/);\n    if (dateMatch) {\n      const day = dateMatch[1].padStart(2, '0');\n      const month = dateMatch[2].padStart(2, '0');\n      let year = dateMatch[3];\n      if (year.length === 2) {\n        year = '20' + year;\n      }\n      dateStr = `${day}.${month}.${year}`;\n    }\n    else {\n      dateMatch = dateStr.match(/^(\\d{4})-(\\d{2})-(\\d{2})$/);\n      if (dateMatch) {\n        dateStr = `${dateMatch[3]}.${dateMatch[2]}.${dateMatch[1]}`;\n      }\n    } \n    \n    return {\n      court: booking.court,\n      date: dateStr,\n      time_start: booking.time_start,\n      time_end: booking.time_end,\n      players_num: parseInt(booking.players_num),\n      notes: booking.notes || ''\n    };\n  });\n  \n  return [{ json: {\n    chat_id,\n    lang,\n    bookings: validatedBookings,\n    booking_count: validatedBookings.length\n  }}];\n  \n} catch (error) {\n  return [{ json: {\n    chat_id,\n    lang,\n    error: error.message,\n    error_key: 'book_invalid_json'\n  }}];\n}"
        },
