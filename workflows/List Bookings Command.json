    {
      "parameters": {
        "jsCode": "const reservations = $input.first().json.result.structuredContent.result;\nconst context = $('Set List Context').first().json;\n\n// Build formatted message for each reservation\nconst lines = reservations.map((r, i) => {\n  const date = r.date || 'N/A';\n  const time = r.time || 'N/A';\n  const club = r.club_name || 'N/A';\n  const club_no = r.club_num || 'N/A';\n  const bookingId = r.booking_id || 'N/A';\n  \n  return `${i + 1}. <b>${date}</b> ${time}\\n  Club: ${club_no} - ${club}\\n  ID: <code>${bookingId}</code>`;\n});\n\nreturn [{\n  json: {\n    chat_id: context.chat_id,\n    lang: context.lang,\n    reservations: reservations,\n    formatted_list: lines.join('\\n\\n'),\n    count: reservations.length\n  }\n}];"
      },
